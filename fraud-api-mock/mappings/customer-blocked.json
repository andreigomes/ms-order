{
  "request": {
    "method": "GET",
    "urlPathPattern": "/api/v1/customers/([^/]+)/blocked"
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "jsonBody": "{{#if (urlPath matches '/api/v1/customers/BLOCKED_CUSTOMER_.*/blocked' or urlPath matches '/api/v1/customers/999999999/blocked')}}true{{else}}false{{/if}}",
    "transformers": ["response-template"]
  }
}

